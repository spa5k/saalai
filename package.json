    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "@vitest/coverage-v8": "^1.3.1"
  }
}